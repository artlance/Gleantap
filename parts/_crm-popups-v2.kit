                                    <defs />
                                    <path fill="#FFBF4B" d="M14.8 17.3H3.2a2.5 2.5 0 01-2.5-2.5V1.5a.8.8 0 01.8-.8h11.7a.8.8 0 01.8.8v8.3c0 1 .7 1.7 1.7 1.7.9 0 1.6.7 1.6 1.7v1.6a2.5 2.5 0 01-2.5 2.5zm0-4.1c-.4 0-.8.3-.8.8v.8a.8.8 0 001.7 0V14c0-.5-.4-.8-.9-.8zm-10-8.4a.8.8 0 000 1.7h5a.8.8 0 100-1.7h-5zm0 3.4a.8.8 0 000 1.6h5a.8.8 0 100-1.6h-5zm0 3.3a.8.8 0 000 1.7h2.5a.8.8 0 100-1.7H4.8z" />
