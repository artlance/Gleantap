<div class="panel panel-w2" data-panel="ongoing">
    <button class="panel-close"><!-- @import "../images/close.svg" --></button>
    <div class="panel-top">
        <div class="ongoing-caption">Ongoing</div>
    </div>
    <!-- /.panel-top -->
    <div class="panel-content">
        <div class="ongoing">
            <label for="ongoing-start-date" class="ongoing-label">Starting on</label>
            <div class="ongoing-row">
                <div class="ongoing-col">
                    <input type="text" class="input-text input-datepicker" value="Oct 20, 2020" id="ongoing-start-date">
                </div>
                <div class="ongoing-col">
                    <select class="select select-time">
                        <option>00:00</option>
                        <option>00:30</option>
                    </select>
                </div>
                <div class="ongoing-col">
                    <select class="select">
                        <option>AM</option>
                        <option>PM</option>
                    </select>
                </div>
            </div>
            <!-- /.ongoing-row -->
            <label for="ongoing-end-date" class="ongoing-label">Ending on</label>
            <div class="ongoing-row">
                <div class="ongoing-col">
                    <input type="text" class="input-text input-datepicker" id="ongoing-end-date">
                </div>
                <div class="ongoing-col">
                    <select class="select select-time">
                        <option>00:00</option>
                        <option>00:30</option>
                    </select>
                </div>
                <div class="ongoing-col">
                    <select class="select">
                        <option>AM</option>
                        <option>PM</option>
                    </select>
                </div>
            </div>
            <!-- /.ongoing-row -->
            <div class="ongoing-note">You can leave out the end date and then your email will continue to be sent until you turn it off</div>
            <div class="ongoing-label">What day to send</div>
            <div class="ongoing-day">
                <select class="select">
                    <option>Specific day of week</option>
                </select>
                <div class="ongoing-day-list">
                    <div class="ongoing-day-item">
                        <input type="checkbox" id="ongoing-day-1" name="ongoing-day-1" checked>
                        <label for="ongoing-day-1">Mo</label>
                    </div>
                    <div class="ongoing-day-item">
                        <input type="checkbox" id="ongoing-day-2" name="ongoing-day-2">
                        <label for="ongoing-day-2">Tu</label>
                    </div>
                    <div class="ongoing-day-item">
                        <input type="checkbox" id="ongoing-day-3" name="ongoing-day-3">
                        <label for="ongoing-day-3">We</label>
                    </div>
                    <div class="ongoing-day-item">
                        <input type="checkbox" id="ongoing-day-4" name="ongoing-day-4" checked>
                        <label for="ongoing-day-4">Th</label>
                    </div>
                    <div class="ongoing-day-item">
                        <input type="checkbox" id="ongoing-day-5" name="ongoing-day-5">
                        <label for="ongoing-day-5">Fr</label>
                    </div>
                    <div class="ongoing-day-item">
                        <input type="checkbox" id="ongoing-day-6" name="ongoing-day-6">
                        <label for="ongoing-day-6">Sa</label>
                    </div>
                    <div class="ongoing-day-item">
                        <input type="checkbox" id="ongoing-day-7" name="ongoing-day-7">
                        <label for="ongoing-day-7">Su</label>
                    </div>
                </div>
                <!-- /.ongoing-day-list -->
            </div>
            <!-- /.ongoing-day -->
            <div class="ongoing-day-select">
                <select class="multi-select" multiple>
                    <option value="1">1</option>
                    <option value="2">2</option>
                    <option value="3">3</option>
                    <option value="4">4</option>
                    <option value="5">5</option>
                    <option value="6">6</option>
                    <option value="7">test long option 0</option>
                    <option value="8">test long option 1</option>
                    <option value="9">test long option 2</option>
                </select>
            </div>
            <label for="ongoing-time" class="ongoing-label">What time to send</label>
            <div class="ongoing-time">
                <div class="ongoing-time-col">
                    <select class="select select-time">
                        <option>00:00</option>
                        <option>00:30</option>
                    </select>
                </div>
                <div class="ongoing-time-col">
                    <select class="select">
                        <option>AM</option>
                        <option>PM</option>
                    </select>
                </div>
            </div>
            <!-- /.ongoing-time -->
            <div class="ongoing-check">
                <div class="checkbox">
                    <input type="checkbox" name="ongoing-check-1" id="ongoing-check-1" checked>
                    <label for="ongoing-check-1">Send time optimization</label>
                </div>
            </div>
        </div>
        <!-- /.ongoing -->
    </div>
    <!-- /.panel-content -->
    <div class="panel-bottom">
        <div class="ongoing-options">
            <div class="ongoing-option"><a href="#" class="button button-secondary panel-cancel">Cancel</a></div>
            <div class="ongoing-option"><a href="#" class="button">Schedule send</a></div>
        </div>
        <!-- /.ongoing-options -->
    </div>
    <!-- /.panel-bottom -->
</div>
