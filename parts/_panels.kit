<div class="panel" data-panel="new-segmentation">
    <button class="panel-close"><!-- @import "../images/close.svg" --></button>
    <div class="panel-top">
        <div class="new-segmentation-caption">New segmentation</div>
    </div>
    <!-- /.panel-top -->
    <div class="panel-content">
        <div class="new-segmentation">
            <div class="new-segmentation-name">
                <label class="new-segmentation-label" for="new-segmentation-name">Segment Name</label>
                <input type="text" class="input-text" id="new-segmentation-name" placeholder="Enter Segment Name">
            </div>
            <div class="new-segmentation-type">
                <div class="new-segmentation-label">Type</div>
                <div class="new-segmentation-type-list">
                    <div class="new-segmentation-type-item">
                        <div class="radio">
                            <input type="radio" id="new-segmentation-type-static" name="new-segmentation-type" checked>
                            <label for="new-segmentation-type-static">Static</label>
                        </div>
                    </div>
                    <div class="new-segmentation-type-item">
                        <div class="radio">
                            <input type="radio" id="new-segmentation-type-dynamic" name="new-segmentation-type">
